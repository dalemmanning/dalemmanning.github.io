---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>Dale Manning</title>
  <link href="http://localhost:4000/"/>
  <link type="application/atom+xml" rel="self" href="http://localhost:4000/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://localhost:4000/</id>
  <author>
    <name>Dale Manning</name>
    <email>dalemmanning@gmail.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://locahost:4000{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://locahost:4000{{ post.url }}"/>
    <title>{{ post.title | xml_escape }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Dale Manning</name>
      <uri>http://localhost:4000/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>